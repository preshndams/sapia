{
  "compilerOptions": {
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "watch": false,
    "target": "es2017",
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true
  },
  //"include": ["src/**/*"],
  "exclude": ["./node_modules/*", ],
}
